{
  "name": "Studies-Teacher-WorkSpace",
  "version": "0.0.1",
  "license": "MIT",
  "description": "your app description",
  "author": "xyz <xyz@gmail.com>",
  "main": "main.js",
  "scripts": {
    "postinstall": "install-app-deps",
    "start": "npm install && electron .",
    "pack": "build --dir",
    "dist": "build"
  },
  "repository": {
    "type": "git",
    "url": " https://github.com/hypemichael/studiesteacher.git"
  },
  "build": {
    "appId": "Studies-Teacher-WorkSpace",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "NSIS",
      "icon": "build/icon.ico"
    }
  },
  "dependencies": {
    "electron-updater": "^2.18.2",
    "mongodb": "^3.0.1"
  },
  "devDependencies": {
    "electron": "latest",
    "electron-builder": "^19.52.1"
  }
}
